    def search_rsid(self, rsid: str) -> Optional[pd.Series]:
        """Search for specific positions in the dataset"""
        try:
            self.logger.warning(f"RSID search not available: {rsid}")
            return None
        except (IndexError, KeyError):
            return None

    def generate_report(self) -> str:
